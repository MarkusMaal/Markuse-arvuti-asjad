<Window x:Class="Markuse_arvuti_lukustamissüsteem.SetPassword"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Markuse_arvuti_lukustamissüsteem"
        mc:Ignorable="d"
        Title="Määra parool" Height="150" Width="550" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" WindowStyle="None">
    <Grid>
        <Label x:Name="TextBoxLabel" Content="Siia läheb mingi tekst kasutaja jaoks." Margin="10,10,10,0" VerticalAlignment="Top" FontSize="20" HorizontalContentAlignment="Center"/>
        <PasswordBox x:Name="PasswordValue" Margin="10,61,10,0" VerticalAlignment="Top" Height="25" HorizontalContentAlignment="Stretch" PasswordChar="*" TabIndex="0" KeyDown="PasswordValue_KeyDown"/>
        <Button x:Name="OKButton" Content="OK" HorizontalAlignment="Right" Margin="0,0,10,12" VerticalAlignment="Bottom" Height="29" Width="63" Click="OKButton_Click" TabIndex="2"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Right" Margin="0,0,90,12" VerticalAlignment="Bottom" Height="29" Width="63" Click="CancelButton_Click" TabIndex="3"/>

    </Grid>
</Window>
